<link rel="icon" type="image/png" sizes="16x16" href={favIcon16x16} />
<link rel="icon" type="image/png" sizes="32x32" href={favIcon32x32} />
<link rel="icon" type="image/png" sizes="96x96" href={favIcon96x96} />
