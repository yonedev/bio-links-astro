import favIcon192x192 from '@favicons/icon-192x192.png'
import favIcon57x57 from '@favicons/icon-57x57.png'
import favIcon60x60 from '@favicons/icon-60x60.png'
import favIcon72x72 from '@favicons/icon-72x72.png'
import favIcon76x76 from '@favicons/icon-76x76.png'
import favIcon114x114 from '@favicons/icon-114x114.png'
import favIcon120x120 from '@favicons/icon-120x120.png'
import favIcon144x144 from '@favicons/icon-144x144.png'
import favIcon152x152 from '@favicons/icon-152x152.png'
